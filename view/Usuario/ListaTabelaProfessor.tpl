<tr cod="[@cod]">
    <td>[@nome]</td>
    <td>[@email]</td>
    <td>[@titulacao]<td>
    <td>[@areaAtuacao]<td>
    <td idUsuario="[@idUsuario]">[@usuario]</td>
    <td>  
        <button type="button" class="btn btn-info editarProfessor" data-toggle="modal" data-target="" cod="[@cod]">  
            <span class="glyphicon glyphicon-pencil"></span>
        </button>
    </td>
    <td>  
        <button type="button" class="btn btn-danger removerProfessor" data-toggle="modal" data-target="#confirma" cod="[@cod]">
            <span class="glyphicon glyphicon-trash"></span>
        </button>
    </td>
</tr>
